docker run --name=my_nginx \
    -p 5000:80 \
    -v /home/yangzheng/my_ui:/home/yangzheng/my_ui \
    -d \
    -e FLASK_ENV="development" \
    -e TZ="Asia/Shanghai" \
    --restart=on-failure:2 \
    python:3.8.2 \
    sh -c \
    "pip3 install -r requirements.txt && " \
    "gunicorn -w 2 --bind 0.0.0.0:80 -k 'gevent' app:app"
